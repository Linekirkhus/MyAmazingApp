<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android = "http://schemas.android.com/apk/res/android"
                                                   xmlns:app = "http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools = "http://schemas.android.com/tools"
                                                   android:id = "@+id/cp_user_create_profile_root_layout"
                                                   android:layout_width = "match_parent"
                                                   android:layout_height = "match_parent"
                                                   android:foregroundTint = "#FFFFFF"
                                                   android:outlineAmbientShadowColor = "#FFFFFF"
                                                   tools:context = ".CreateProfileActivity"
    >


	<com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id = "@+id/fab_btn_cp_update_profile_image"
        android:layout_width = "52dp"
        android:layout_height = "53dp"
        android:layout_gravity = "end|bottom"
        android:layout_margin = "16dp"
        android:contentDescription = "edit profile photo"
        android:src = "@drawable/ic_baseline_edit_24"
        app:backgroundTint = "@color/davidoff"
        app:layout_constraintBottom_toBottomOf = "parent"
        app:layout_constraintEnd_toEndOf = "parent"
        app:layout_constraintHorizontal_bias = "0.281"
        app:layout_constraintStart_toStartOf = "parent"
        app:layout_constraintTop_toTopOf = "parent"
        app:layout_constraintVertical_bias = "0.173"
        /><ImageView
    android:id = "@+id/iv_cp_profile_picture"
    android:layout_width = "150dp"
    android:layout_height = "154dp"
    android:contentDescription = "TODO"
    app:layout_constraintBottom_toTopOf = "@+id/linearLayout2"
    app:layout_constraintEnd_toStartOf = "@+id/linearLayout"
    app:layout_constraintHorizontal_bias = "0.516"
    app:layout_constraintStart_toStartOf = "parent"
    app:layout_constraintTop_toTopOf = "parent"
    app:layout_constraintVertical_bias = "0.372"
    app:srcCompat = "@drawable/userphoto_1"
    />


	<LinearLayout
        android:id = "@+id/linearLayout"
        android:layout_width = "230dp"
        android:layout_height = "196dp"
        android:orientation = "vertical"
        app:layout_constraintBottom_toBottomOf = "parent"
        app:layout_constraintEnd_toEndOf = "parent"
        app:layout_constraintHorizontal_bias = "1.0"
        app:layout_constraintStart_toStartOf = "parent"
        app:layout_constraintTop_toTopOf = "parent"
        app:layout_constraintVertical_bias = "0.0"
        >
            <TextView
                android:id = "@+id/tv_cp_user_name"
                android:layout_width = "match_parent"
                android:layout_height = "41dp"
                android:background = "@color/semi_transparent_greeinish"
                android:fontFamily = "@font/montserrat"
                android:lineHeight = "4sp"
                android:lineSpacingExtra = "10sp"
                android:paddingLeft = "6dp"
                android:paddingTop = "4dp"
                android:text = "Name"
                android:textColor = "#FFFFFF"
                android:textSize = "14sp"
                android:textStyle = "normal"
                />

        <EditText
            android:id = "@+id/et_cp_user_name"
            android:layout_width = "match_parent"
            android:layout_height = "wrap_content"
            android:autofillHints = ""
            android:ems = "10"
            android:inputType = "text"
            android:text = ""
            />


		<TextView
            android:id = "@+id/tv_cp_user_location"
            android:layout_width = "match_parent"
            android:layout_height = "47dp"
            android:background = "@color/semi_transparent_greeinish"
            android:fontFamily = "@font/montserrat"
            android:lineHeight = "4sp"
            android:lineSpacingExtra = "10sp"
            android:paddingLeft = "6dp"
            android:paddingTop = "4dp"
            android:text = "Location"
            android:textColor = "#FFFFFF"
            android:textSize = "14sp"
            android:textStyle = "normal"
            />

        <EditText
            android:id = "@+id/et_cp_user_location"
            android:layout_width = "match_parent"
            android:layout_height = "60dp"
            android:autofillHints = ""
            android:ems = "10"
            android:inputType = "text"
            android:text = ""
            />

    </LinearLayout >


	<LinearLayout
        android:id = "@+id/linearLayout2"
        android:layout_width = "411dp"
        android:layout_height = "160dp"
        android:orientation = "vertical"
        app:layout_constraintBottom_toBottomOf = "parent"
        app:layout_constraintEnd_toEndOf = "parent"
        app:layout_constraintHorizontal_bias = "0.486"
        app:layout_constraintStart_toStartOf = "parent"
        app:layout_constraintTop_toBottomOf = "@+id/linearLayout"
        app:layout_constraintVertical_bias = "0.002"
        >

            <TextView
                android:id = "@+id/tv_cp_user_bio"
                android:layout_width = "match_parent"
                android:layout_height = "36dp"
                android:background = "@color/semi_transparent_greeinish"
                android:fontFamily = "@font/montserrat"
                android:lineHeight = "4sp"
                android:lineSpacingExtra = "10sp"
                android:paddingStart = "6dp"
                android:paddingTop = "4dp"
                android:text = "Bio"
                android:textColor = "#FFFFFF"
                android:textSize = "14sp"
                android:textStyle = "normal"
                />
    <EditText
        android:id = "@+id/et_cp_user_bio"
        android:layout_width = "match_parent"
        android:layout_height = "113dp"
        android:ems = "10"
        android:gravity = "start|top"
        android:inputType = "textMultiLine"
        />
	</LinearLayout >
	<LinearLayout
        android:id = "@+id/linearLayout3"
        android:layout_width = "409dp"
        android:layout_height = "144dp"
        android:orientation = "vertical"
        app:layout_constraintBottom_toBottomOf = "parent"
        app:layout_constraintEnd_toEndOf = "parent"
        app:layout_constraintHorizontal_bias = "0.4"
        app:layout_constraintStart_toStartOf = "parent"
        app:layout_constraintTop_toBottomOf = "@+id/tv_cp_user_info"
        app:layout_constraintVertical_bias = "0.0"
        >
            <LinearLayout
                android:layout_width = "match_parent"
                android:layout_height = "133dp"
                >

            <LinearLayout
                android:layout_width = "139dp"
                android:layout_height = "match_parent"
                android:orientation = "vertical"
                >
            <TextView
                android:id = "@+id/tv_cp_user_profession"
                android:layout_width = "match_parent"
                android:layout_height = "45dp"
                android:paddingStart = "10sp"
                android:paddingTop = "10sp"
                android:paddingEnd = "10sp"
                android:text = "Profession"
                />

				<TextView
                    android:id = "@+id/tv_cp_user_age"
                    android:layout_width = "match_parent"
                    android:layout_height = "44dp"
                    android:paddingStart = "10sp"
                    android:paddingTop = "10sp"
                    android:paddingEnd = "10sp"
                    android:text = "Age"
                    />

				<TextView
                    android:id = "@+id/tv_cp_user_gender"
                    android:layout_width = "match_parent"
                    android:layout_height = "41dp"
                    android:paddingStart = "10sp"
                    android:paddingTop = "10sp"
                    android:paddingEnd = "10sp"
                    android:text = "Gender"
                    /></LinearLayout ><LinearLayout
                android:layout_width = "261dp"
                android:layout_height = "136dp"
                android:orientation = "vertical"
                >
            <EditText
                android:id = "@+id/et_cp_user_profession"
                android:layout_width = "match_parent"
                android:layout_height = "45dp"
                android:ems = "10"
                android:inputType = "textPersonName"
                android:text = ""
                />

				<EditText
                    android:id = "@+id/et_cp_user_age"
                    android:layout_width = "match_parent"
                    android:layout_height = "45dp"
                    android:ems = "10"
                    android:inputType = "textPersonName"
                    />

				<EditText
                    android:id = "@+id/et_cp_user_gender"
                    android:layout_width = "match_parent"
                    android:layout_height = "45dp"
                    android:ems = "10"
                    android:inputType = "textPersonName"
                    android:text = ""
                    />
			</LinearLayout >

			</LinearLayout >
	</LinearLayout >

	<TextView
        android:id = "@+id/tv_cp_user_info"
        android:layout_width = "393dp"
        android:layout_height = "36dp"
        android:background = "@color/semi_transparent_greeinish"
        android:fontFamily = "@font/montserrat"
        android:lineHeight = "4sp"
        android:lineSpacingExtra = "10sp"
        android:paddingStart = "6dp"
        android:paddingTop = "4dp"
        android:text = "@string/info"
        android:textColor = "#FFFFFF"
        android:textSize = "14sp"
        android:textStyle = "normal"
        app:layout_constraintBottom_toBottomOf = "parent"
        app:layout_constraintEnd_toEndOf = "parent"
        app:layout_constraintHorizontal_bias = "0.0"
        app:layout_constraintStart_toStartOf = "parent"
        app:layout_constraintTop_toBottomOf = "@+id/linearLayout2"
        app:layout_constraintVertical_bias = "0.0"
        />
	<LinearLayout
		android:id = "@+id/linearLayout4"
		android:layout_width = "414dp"
		android:layout_height = "185dp"
		android:orientation = "vertical"
		app:layout_constraintBottom_toBottomOf = "parent"
		app:layout_constraintEnd_toEndOf = "parent"
		app:layout_constraintStart_toStartOf = "parent"
		app:layout_constraintTop_toBottomOf = "@+id/linearLayout3"
		app:layout_constraintVertical_bias = "0.060000002"
		>
            <TextView
				android:id = "@+id/tv_cp_user_contact"
				android:layout_width = "match_parent"
				android:layout_height = "37dp"
				android:background = "@color/semi_transparent_greeinish"
				android:fontFamily = "@font/montserrat"
				android:lineHeight = "4sp"
				android:lineSpacingExtra = "10sp"
				android:paddingTop = "4dp"
				android:text = "@string/info"
				android:textColor = "#FFFFFF"
				android:textSize = "14sp"
				android:textStyle = "bold"
				/><LinearLayout
		android:layout_width = "match_parent"
		android:layout_height = "161dp"
		android:orientation = "horizontal"
		>

        <LinearLayout
			android:layout_width = "171dp"
			android:layout_height = "match_parent"
			android:orientation = "vertical"
			>
    <TextView
		android:id = "@+id/tv_cp_user_interests"
		android:layout_width = "match_parent"
		android:layout_height = "45dp"
		android:paddingStart = "10sp"
		android:paddingTop = "10sp"
		android:paddingEnd = "10sp"
		android:text = "@string/interests"
		/><TextView
			android:id = "@+id/tv_cp_user_favorite_movie"
			android:layout_width = "match_parent"
			android:layout_height = "45dp"
			android:paddingStart = "10sp"
			android:paddingTop = "10sp"
			android:paddingEnd = "10sp"
			android:text = "@string/favorite_movie"
			/><TextView
			android:id = "@+id/tv_cp_user_favorite_genre"
			android:layout_width = "match_parent"
			android:layout_height = "45dp"
			android:paddingStart = "10sp"
			android:paddingTop = "10sp"
			android:paddingEnd = "10sp"
			android:text = "Favorite Genre"
			/></LinearLayout >




		<LinearLayout
			android:layout_width = "238dp"
			android:layout_height = "match_parent"
			android:orientation = "vertical"
			>
    <EditText
		android:id = "@+id/et_cp_user_interests"
		android:layout_width = "match_parent"
		android:layout_height = "45dp"
		android:ems = "10"
		android:inputType = "textPersonName"
		android:text = ""
		/>
	<EditText
		android:id = "@+id/et_cp_user_favorite_movie"
		android:layout_width = "match_parent"
		android:layout_height = "45dp"
		android:ems = "10"
		android:inputType = "textPersonName"
		android:text = ""
		/><EditText
			android:id = "@+id/et_cp_user_favorite_genre"
			android:layout_width = "match_parent"
			android:layout_height = "45dp"
			android:ems = "10"
			android:inputType = "textPersonName"
			android:text = ""
			/>

	</LinearLayout >

	</LinearLayout >
	</LinearLayout ><Button
	android:id = "@+id/cp_btn_save_profile"
	android:layout_width = "180dp"
	android:layout_height = "53dp"
	android:text = "Save Changes"
	app:layout_constraintBottom_toBottomOf = "parent"
	app:layout_constraintEnd_toEndOf = "parent"
	app:layout_constraintStart_toStartOf = "parent"
	app:layout_constraintTop_toBottomOf = "@+id/linearLayout4"
	/>

</androidx.constraintlayout.widget.ConstraintLayout >